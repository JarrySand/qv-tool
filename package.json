{
  "name": "qv-tool",
  "version": "0.1.0",
  "description": "A modern Quadratic Voting tool for fair and expressive decision-making",
  "keywords": [
    "quadratic-voting",
    "voting",
    "democracy",
    "next.js",
    "typescript",
    "prisma"
  ],
  "homepage": "https://github.com/username/qv-tool#readme",
  "bugs": {
    "url": "https://github.com/username/qv-tool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/qv-tool.git"
  },
  "license": "MIT",
  "author": "QV-Tool Contributors",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
