    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.554.0",
    "next": "16.0.4",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.5",
    "pg": "^8.16.3",
